{
  "name": "@opuscapita/react-autocompletes",
  "version": "1.0.5",
  "description": "React ",
  "scripts": {
    "npm-build": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0,react --plugins transform-decorators-legacy --ignore __tests__,*.spec.js,demo src/client/components --out-dir lib",
    "link-mode": "webpack --watch --config ./webpack.link.config.js",
    "lint": "eslint src/client",
    "start": "node www/index",
    "npm-publish": "npm run npm-build && npm-publish",
    "publish-snapshot": "npm run npm-publish",
    "publish-release": "npm run npm-publish -- --release"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "repository": "OpusCapitaBES/react-autocompletes",
  "license": "Apache-2.0",
  "readme": "README.md",
  "peerDependencies": {
    "react": ">= 0.14.0 || >= 15.0.0",
    "react-dom": ">= 0.14.0 || >= 15.0.0"
  },
  "devDependencies": {
    "@opuscapita/npm-scripts": "1.0.8",
    "@opuscapita/react-showroom-client": "1.2.2",
    "@opuscapita/react-showroom-server": "1.2.0",
    "autoprefixer": "6.4.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.18.0",
